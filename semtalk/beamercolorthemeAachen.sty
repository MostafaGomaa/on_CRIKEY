\mode<presentation>

\setbeamercolor*{palette primary}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor*{palette secondary}{use=structure,fg=white,bg=structure.fg!75!black}
\setbeamercolor*{palette tertiary}{use=structure,fg=white,bg=structure.fg!50!black}
\setbeamercolor*{palette quaternary}{fg=white,bg=black}

\setbeamercolor*{sidebar}{use=structure,bg=structure.fg}
  
\setbeamercolor*{palette sidebar primary}{use=structure,fg=structure.fg!10}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{use=structure,fg=structure.fg!50}
\setbeamercolor*{palette sidebar quaternary}{fg=white}

\setbeamercolor*{titlelike}{parent=palette primary}

\setbeamercolor*{separation line}{fg=black}
\setbeamercolor*{fine separation line}{}

%% BLOCK BODY COLORS
\setbeamercolor*{block body}{bg=normal text.bg!97!black}
\setbeamercolor*{block body alerted}{bg=normal text.bg!97!black}
\setbeamercolor*{block body example}{bg=normal text.bg!97!black}
%% BLOCK TITLE COLORS
%\setbeamercolor*{block title}{parent=structure,bg=structure.fg!27!black,fg=blue}
\setbeamercolor*{block title}{use={normal text,alerted text},fg=normal text.fg!75!normal text.fg,bg=normal text.bg!78!black,fg=blue!75!black}
\setbeamercolor*{block title alerted}{use={normal text,alerted text},fg=alerted text.fg!75!normal text.fg,bg=normal text.bg!78!black}
\setbeamercolor*{block title example}{use={normal text,example text},fg=example text.fg!75!normal text.fg,bg=normal text.bg!78!black}


\mode
<all>
